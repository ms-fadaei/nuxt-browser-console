{
  "name": "nuxt-browser-console",
  "version": "2.0.0-beta.1",
  "description": "show server-side & clinet-side logs/errors/warnings inside browser console",
  "repository": "git://github.com/ms-fadaei/nuxt-browser-console.git",
  "license": "MIT",
  "keywords": [
    "nuxt",
    "nuxt-modules",
    "console-log"
  ],
  "author": {
    "name": "Mohammad Saleh Fadaei",
    "email": "ms.fadaei1997@gmail.com",
    "url": "https://twitter.com/ms_fadaei"
  },
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "module": "./dist/module.mjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "lint": "eslint --ext .js,.ts,.vue --ignore-path .gitignore .",
    "lint:fix": "yarn lint --fix"
  },
  "dependencies": {
    "@nuxt/kit": "3.0.0"
  },
  "devDependencies": {
    "@nuxt/module-builder": "0.2.1",
    "@nuxtjs/eslint-config-typescript": "12.0.0",
    "eslint": "8.28.0",
    "nuxt": "3.0.0",
    "standard-version": "9.5.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
